@model IEnumerable<Brand>
@{
    ViewBag.Title = "Список марок";
}

<h6 class="bg-primary text-white text-center m-2 p-2">Таблица продуктов</h6>
<div class="m-2">
    <table class="table table-sm table-striped table-bordered">
        <thead><tr><th>Название</th><th></th></tr></thead>
        <tbody>
            @foreach (Brand p in Model)
            {
                    <partial name="_RowPartial" model="p" />
            }
        </tbody>
    </table>
    <a asp-page="/suppliers/list" class="btn btn-secondary">Suppliers</a>
</div>